{
  "name": "@poc/auth_front",
  "version": "1.0.0",
  "scripts": {
    "build": "webpack --mode production",
    "build:css": "yarn generate:css --minify",
    "build:dev": "webpack --mode development",
    "build-storybook": "build-storybook",
    "clean": "rm -rf dist",
    "dev": "run-p \"dev:*\"",
    "dev:live": "webpack serve --mode development --live-reload --hot",
    "dev:types": "watch 'make-federated-types' ./src",
    "dev:css": "yarn generate:css --watch",
    "generate:css": "npx tailwindcss -c tailwind.config.cjs -i ./styles/tailwind.css -o ./src/tailwind.css",
    "start": "cd dist && PORT=8081 npx serve",
    "storybook": "run-p \"storybook:*\"",
    "storybook:css": "yarn dev:css",
    "storybook:mock": "start-storybook -p 6006 -s public"
  },
  "license": "MIT",
  "dependencies": {
    "@poc/query": "1.0.0",
    "clsx": "^1.2.1",
    "history": "5.3.0",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.0",
    "serve": "14.0.1",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@babel/core": "7.18.9",
    "@babel/plugin-transform-runtime": "7.18.9",
    "@babel/preset-env": "7.18.9",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.18.6",
    "@pixability-ui/federated-types": "^0.2.0",
    "@poc/auth_database": "*",
    "@poc/auth_mocks": "*",
    "@poc/ports-map": "*",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/builder-webpack5": "^6.5.12",
    "@storybook/manager-webpack5": "^6.5.12",
    "@storybook/react": "^6.5.12",
    "@storybook/testing-library": "^0.0.13",
    "@tailwindcss/forms": "0.5.3",
    "@types/react": "18.0.19",
    "@types/react-dom": "18.0.6",
    "assert": "^2.0.0",
    "autoprefixer": "10.4.8",
    "babel-loader": "8.2.5",
    "css-loader": "6.7.1",
    "html-webpack-plugin": "5.5.0",
    "msw": "^0.47.4",
    "msw-storybook-addon": "^1.6.3",
    "postcss": "8.4.16",
    "postcss-loader": "4.3.0",
    "style-loader": "3.3.1",
    "timers-browserify": "2.0.12",
    "typescript": "4.8.3",
    "tailwindcss": "3.2.4",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.9.3"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
