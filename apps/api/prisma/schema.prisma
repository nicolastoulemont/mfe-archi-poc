generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Contact {
  id       Int     @id @default(autoincrement())
  first    String?
  last     String?
  avatar   String?
  twitter  String?
  notes    String?
  favorite Boolean @default(false)
}
