{
  "name": "shell",
  "version": "1.0.0",
  "scripts": {
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "start": "cd dist && PORT=8080 npx serve",
    "dev": "concurrently \"yarn dev:types\" \"yarn dev:start\"",
    "dev:start": "webpack serve --open --mode development",
    "dev:live": "webpack serve --open --mode development --live-reload --hot",
    "dev:types": "make-federated-types",
    "clean": "rm -rf dist"
  },
  "license": "MIT",
