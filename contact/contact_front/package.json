{
  "name": "@poc/contact_front",
  "version": "1.0.0",
  "private": true,
  "workspaces": {
    "nohoist": [
      "match-sorter"
    ]
  },
  "scripts": {
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "start": "cd dist && PORT=8081 npx serve",
    "dev": "concurrently \"yarn dev:types\" \"yarn dev:start\"",
    "dev:start": "webpack serve --mode development",
    "dev:live": "webpack serve --mode development --live-reload --hot",
    "dev:types": "watch 'make-federated-types' ./src",
    "storybook": "start-storybook -p 6006",
    "storybook:mock": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook",
    "clean": "rm -rf dist"
  },
  "license": "MIT",
  "dependencies": {
    "@poc/query": "1.0.0",
    "history": "5.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.0",
    "match-sorter": "^6.3.1"
  },
  "devDependencies": {
    "@babel/core": "7.18.9",
    "@babel/plugin-transform-runtime": "7.18.9",
    "@babel/preset-env": "7.18.9",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.18.6",
    "@pixability-ui/federated-types": "^0.2.0",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/builder-webpack5": "^6.5.12",
    "@storybook/manager-webpack5": "^6.5.12",
    "@storybook/react": "^6.5.12",
    "@storybook/testing-library": "^0.0.13",
    "@types/react": "18.0.19",
    "@types/react-dom": "18.0.6",
    "autoprefixer": "10.4.8",
    "babel-loader": "8.2.5",
    "concurrently": "^7.4.0",
    "css-loader": "6.7.1",
    "html-webpack-plugin": "5.5.0",
    "msw": "^0.47.4",
    "msw-storybook-addon": "^1.6.3",
    "postcss": "8.4.16",
    "postcss-loader": "4.3.0",
    "serve": "14.0.1",
    "style-loader": "3.3.1",
    "typescript": "4.8.3",
    "watch": "^1.0.2",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.11.1",
    "@poc/ports-map": "*"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
